<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Ocean Theme -->
    
    <Color x:Key="BackgroundColor">#0D1117</Color>
    <Color x:Key="SurfaceColor">#161B22</Color>
    <Color x:Key="TextPrimaryColor">#C9D1D9</Color>
    <Color x:Key="TextSecondaryColor">#8B949E</Color>
    <Color x:Key="BorderColor">#30363D</Color>
    <Color x:Key="AccentColor">#58A6FF</Color>
    <Color x:Key="AccentHoverColor">#79C0FF</Color>
    <Color x:Key="ErrorColor">#F85149</Color>
    <Color x:Key="SuccessColor">#3FB950</Color>
    
    <!-- Brushes -->
    <SolidColorBrush x:Key="BackgroundBrush" Color="#0D1117" />
    <SolidColorBrush x:Key="SurfaceBrush" Color="#161B22" />
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="#C9D1D9" />
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="#8B949E" />
    <SolidColorBrush x:Key="BorderBrush" Color="#30363D" />
    <SolidColorBrush x:Key="AccentBrush" Color="#58A6FF" />
    <SolidColorBrush x:Key="AccentHoverBrush" Color="#79C0FF" />
    <SolidColorBrush x:Key="ErrorBrush" Color="#F85149" />
    <SolidColorBrush x:Key="SuccessBrush" Color="#3FB950" />
    
</ResourceDictionary>
